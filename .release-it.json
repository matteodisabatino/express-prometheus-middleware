{
  "git": {
    "commit": true,
    "push": true,
    "tag": true
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": true
  },
  "publishConfig": {
    "access": "public"
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}