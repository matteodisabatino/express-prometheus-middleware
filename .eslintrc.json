{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:ava/recommended",
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "ava/no-ignored-test-files": "off"
  }
}